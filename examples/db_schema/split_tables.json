{
  "split": {
    "action": "transfer",
    "origin": {
      "smt": "json|./|db_schema.json|*"
    },
    "terminal": {
      "smt": "split|*|*|*",
      "options": {
        "splitOn": "TABLE_NAME",
        "encoding": "./db_schema.encoding.json"
      },
      "tract": {
        "terminal": {
          "smt": "json|./|table_*|*",
          "options": {}
        }
      }
    }
  }
}
