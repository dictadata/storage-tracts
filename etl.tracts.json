{
  "tract-name": {
    "origin": {
      "smt": "<smt|urn>",
      "options": {}
    },
    "transform": {
      "filter": {},
      "select": {}
    },
    "terminal": {
      "smt": "json|./|foofile.json|*",
      "options": {
        "encoding": "<schema_name.encoding.json>"
      }
    }
  },
  "_config": {
    "codex": {
