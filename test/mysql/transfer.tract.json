{
  "domain": "foo",
  "name": "transfer.tract.json",
  "type": "tract",
  "actions": [
    {
    "name": "csv-table",
    "action": "transfer",
    "origin": {
      "smt": "csv|./test/data/input/|foofile.csv|*",
      "options": {
        "header": true,
        "encoding": "./test/data/input/engrams/foo_schema.engram.json"
      }
    },
    "terminal": {
      "smt": {
        "model": "mysql",
        "locus": "host=dev.dictadata.net;database=storage_node",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  {
    "name": "table-csv",
    "action": "transfer",
    "origin": {
      "smt": "foo:mysql-foo_schema"
    },
    "terminal": {
      "smt": "csv|./test/data/output/mysql/|foofile.csv|*",
      "options": {
        "header": true
      }
    }
  },
  {
    "name": "bulkload",
    "action": "transfer",
    "origin": {
      "smt": "csv|/var/dictadata/test/data/input/|timeseries.csv|*",
      "options": {
        "encoding": {
          "fields": {
            "time": "date",
            "temp": "number"
          }
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "mysql",
        "locus": "host=dev.dictadata.net;database=storage_node",
        "schema": "timeseries",
        "key": "*"
      },
      "options": {
        "bulkLoad": false
      }
    }
  }
]
}
