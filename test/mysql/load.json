        }
      },
      "terminal": {
        "smt": "mysql|host=localhost;user=dicta;password=data;database=storage_node|foo_load|*",
        "options": {
          "encoding": "./test/data/input/foo_schema_01.encoding.json"
        }
      }
    }
  }
}
