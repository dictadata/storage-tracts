{
  "load": {
    "action": "scan",
    "origin": {
      "smt": "json|./data/input/|foofile*.json|*",
      "options": {
        "recursive": false
      }
    },
    "schema": {
      "action": "transfer",
      "origin": {
        "smt": "json|./data/input/|${name}|*",
        "options": {
          "encoding": "./data/input/encodings/foo_schema_01.encoding.json"
        }
      },
      "terminal": {
        "smt": "mysql|host=dev.dictadata.net;database=storage_node|foo_load|=Foo",
        "options": {
          "encoding": "./data/input/encodings/foo_schema_01.encoding.json"
        }
      }
    }
  }
}
