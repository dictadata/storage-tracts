{
  "csv-table": {
    "action": "transfer",
    "origin": {
      "smt": "csv|./test/data/input/|foofile.csv|*",
      "options": {
        "header": true
      }
    },
    "terminal": {
      "smt": {
        "model": "mysql",
        "locus": "host=localhost;user=dicta;password=data;database=storage_node",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  "table-csv": {
    "action": "transfer",
    "origin": {
      "smt": "mysql-foo_schema"
    },
    "terminal": {
      "smt": "csv|./test/data/output/mysql/|foofile.csv|*",
      "options": {
        "header": true
      }
    }
  },
  "bulkload": {
    "action": "transfer",
    "origin": {
      "smt": "csv|./test/data/input/|timeseries.csv|*",
      "options": {
        "encoding": {
          "time": "date",
          "temp": "number"
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "mysql",
        "locus": "host=localhost;user=dicta;password=data;database=storage_node",
        "schema": "timeseries",
        "key": "*"
      },
      "options": {
        "bulkLoad": false
      }
    }
  }
}
