{
  "scan": {
    "action": "scan",
    "origin": {
      "smt": "json|zip:./data/input/foofile.zip|foofile*.json|*",
      "options": {
        "recursive": true
      }
    },
    "schema": {
      "action": "codify",
      "origin": {
        "smt": "json|zip:./data/input/foofile.zip|${name}|*",
        "options": {
          "encoding": "./data/output/zip/scan.encoding.json",
          "max_read": 100
        }
      },
      "terminal": {
        "output": "./data/output/zip/scan.encoding.json"
      }
    }
  }
}
