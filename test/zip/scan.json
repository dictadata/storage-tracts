{
  "scan": {
    "action": "foreach",
    "origin": {
      "smt": "json|zip:./test/data/input/foofile.zip|foofile*.json|*",
      "options": {
        "recursive": true,
        "replace_terminal_smt": false
      }
    },
    "schema": {
      "action": "codify",
      "origin": {
        "smt": "json|zip:./test/data/input/foofile.zip|*|*",
        "options": {
          "encoding": "./test/data/output/zip/scan.encoding.json",
          "max_read": 100
        }
      },
      "terminal": {
        "output": "./test/data/output/zip/scan.encoding.json"
      }
    }
  }
}
