{
  "realm": "",
  "name": "add.tract",
  "type": "tract",
  "config": {
    "engrams": {
      "smt": "elasticsearch|http://dev.dictadata.net:9200/|etl_engrams|*",
      "options": {
        "description": "test"
      }
    }
  },
  "actions": [
    {
      "name": "engrams",
      "action": "engrams",
      "store": [
        {
          "engram": "./test/data/input/engrams/foo_schema.engram.json",
          "name": "jsonfile_foo_schema",
          "smt": "json|./test/data/input/|foofile.json|*"
        },
        {
          "engram": "./test/data/input/engrams/foo_widgets.engram.json",
          "name": "json_widgets",
          "smt": "json|./test/data/input/|foo_widgets.json|*"
        },
        {
          "engram": "./test/data/input/engrams/foo_schema.engram.json",
          "name": "es_foo_schema",
          "smt": "elasticsearch|http://dev.dictadata.net:9200|foo_schema|!Foo",
          "options": {
            "refresh": true
          },
          "tags": [ "bar" ]
        },
        {
          "engram": "./test/data/input/engrams/foo_schema.engram.json",
          "name": "es_foo_empty",
          "smt": "elasticsearch|http://dev.dictadata.net:9200|foo_empty|!Foo",
          "options": {
            "refresh": true
          },
          "source": "foo:es_foo_schema"
        },
        {
          "engram": "./test/data/input/engrams/foo_schema.engram.json",
          "name": "mysql_foo_schema",
          "smt": "mysql|host=dev.dictadata.net;database=storage_node|foo_schema|=Foo",
          "tags": [ "bar" ]
        },
        {
          "engram": "./test/data/input/engrams/tl_YYYY_us_state.engram.json",
          "name": "tl_2023_us_state",
          "smt": "elasticsearch|http://dev.dictadata.net:9200/|tl_2023_us_state|*"
        }
      ]
    }
  ]
}
