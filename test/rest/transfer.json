{
  "us_national_fed_codes": {
    "descripion": "transfer NationalFedCodes to Elasticsearch",
    "action": "scan",
    "origin": {
      "smt": "*|zip:/var/data/US/usgs.gov/NationalFedCodes.zip/|*.txt|*",
      "options": {
        "recursive": false
      }
    },
    "transfer": {
      "action": "transfer",
      "origin": {
        "smt": "csv|zip:/var/data/US/usgs.gov/NationalFedCodes.zip|${rpath}|*",
        "options": {
          "header": true,
          "separator": "|",
          "encoding": "./encodings/us_national_fed_codes.encoding.json"
        }
      },
      "terminal": {
        "smt": "us_national_fed_codes"
      }
    }
  },
  "rest-json": {
    "action": "transfer",
    "origin": {
      "smt": "rest|https://api.weather.gov/gridpoints/DVN/34,71/|forecast|=*",
      "options": {
        "http": {
          "headers": {
            "Accept": "application/ld+json",
            "User-Agent": "@dictadata.org/storage-node (info@dictadata.org)"
          }
        },
        "extract": "periods"
      }
    },
    "terminal": {
      "smt": "json|./test/data/output/rest/|transfer.json|*",
      "output": "./test/data/output/rest/transfer.json",
      "compareValues": 1
    }
  }
}
