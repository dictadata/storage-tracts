{
  "forecast": {
    "action": "transfer",
    "origin": {
      "smt": "rest|https://api.weather.gov/gridpoints/DVN/34,71/|forecast|=*",
      "options": {
        "http": {
          "headers": {
            "Accept": "application/ld+json",
            "User-Agent": "@dictadata.org/storage-node (info@dictadata.org)"
          }
        },
        "extract": "periods"
      }
    },
    "transform": {
      "select": {
        "fields": [
          "name",
          "temperature",
          "temperatureUnit",
          "windSpeed",
          "windDirection",
          "shortForecast"
        ]
      }
    },
    "terminal": {
      "smt": "json|./data/output/rest/|transform.json|*",
      "output": "./data/output/rest/transform.json",
      "compareValues": 1
    }
  }
}
