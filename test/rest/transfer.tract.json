{
  "domain": "foo",
  "name": "transfer.tract.json",
  "type": "tract",
  "actions": [
    {
      "name": "rest-json",
      "action": "transfer",
      "origin": {
        "smt": "rest|https://api.weather.gov/gridpoints/DVN/34,71/|forecast|=*",
        "options": {
          "http": {
            "headers": {
              "Accept": "application/ld+json",
              "User-Agent": "@dictadata.net/storage-node (info@dictadata.net)"
            }
          },
          "retries": 1,
          "extract": "periods"
        }
      },
      "terminal": {
        "smt": "json|./test/data/output/rest/|transfer.json|*",
        "output": "./test/data/output/rest/transfer.json",
        "compareValues": 1
      }
    }
  ]
}
