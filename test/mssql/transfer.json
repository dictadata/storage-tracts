{
  "csv-table": {
    "action": "transfer",
    "origin": {
      "smt": "csv|./data/input/|foofile.csv|*",
      "options": {
        "header": true,
        "encoding": "./data/input/foo_schema.encoding.json"
      }
    },
    "terminal": {
      "smt": {
        "model": "mssql",
        "locus": "server=localhost;username=dicta;password=data;database=storage_node",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  "table-csv": {
    "action": "transfer",
    "origin": {
      "smt": "mssql-foo_schema"
    },
    "terminal": {
      "smt": "csv|./data/output/mssql/|foofile.csv|*",
      "options": {
        "header": true
      }
    }
  },
  "bulkload": {
    "action": "transfer",
    "origin": {
      "smt": "csv|./data/input/|timeseries.csv|*",
      "options": {
        "encoding": {
          "time": "date",
          "temp": "number"
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "mssql",
        "locus": "server=localhost;username=dicta;password=data;database=storage_node",
        "schema": "timeseries",
        "key": "*"
      },
      "options": {
        "bulkLoad": true
      }
    }
  }
}
