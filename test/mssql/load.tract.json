{
  "domain": "",
  "name": "load.tract",
  "type": "tract",
  "actions": [
    {
      "name": "load",
      "action": "scan",
      "origin": {
        "smt": "json|./test/data/input/|foofile*.json|*",
        "options": {
          "recursive": false
        }
      },
      "actions": [
        {
          "name": "schema",
          "action": "transfer",
          "origin": {
            "smt": "json|./test/data/input/|${name}|*",
            "options": {
              "encoding": "./test/data/input/engrams/foo_schema_01.engram.json"
            }
          },
          "terminal": {
            "smt": "mssql|server=dev.dictadata.net;database=storage_node|foo_load|=Foo",
            "options": {
              "encoding": "./test/data/input/engrams/foo_schema_01.engram.json"
            }
          }
        }
      ]
    }
  ]
}