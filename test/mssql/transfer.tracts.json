{
  "domain": "foo",
  "name": "transfer.tracts.json",
  "type": "tract",
  "tracts": [
    {
      "name": "csv-table",
      "action": "transfer",
      "origin": {
        "smt": "csv|./test/data/input/|foofile.csv|*",
        "options": {
          "header": true,
          "encoding": "./test/data/input/encodings/foo_schema.encoding.json"
        }
      },
      "terminal": {
        "smt": {
          "model": "mssql",
          "locus": "server=dev.dictadata.net;database=storage_node",
          "schema": "foo_schema_etl",
          "key": "*"
        }
      }
    },
    {
      "name": "table-csv",
      "action": "transfer",
      "origin": {
        "smt": "foo:mssql-foo_schema"
      },
      "terminal": {
        "smt": "csv|./test/data/output/mssql/|foofile.csv|*",
        "options": {
          "header": true
        }
      }
    },
    {
      "name": "bulkload",
      "action": "transfer",
      "origin": {
        "smt": "csv|/var/dictadata/test/data/input/|timeseries.csv|*",
        "options": {
          "encoding": {
            "time": "date",
            "temp": "number"
          }
        }
      },
      "terminal": {
        "smt": {
          "model": "mssql",
          "locus": "server=dev.dictadata.net;database=storage_node",
          "schema": "timeseries",
          "key": "*"
        },
        "options": {
          "bulkLoad": true
        }
      }
    }
  ]
}
