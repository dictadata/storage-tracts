{
  "domain": "",
  "name": "transform.tract",
  "type": "tract",
  "actions": [
    {
      "name": "csv-json",
      "action": "transfer",
      "origin": {
        "smt": "csv|./test/data/input/|foofile.csv|*",
        "options": {
          "header": true
        }
      },
      "transforms": [
        {
          "transform": "filter",
          "match": {
            "Bar": "row"
          },
          "drop": {
            "Baz": 456
          }
        },
        {
          "transform": "mutate",
          "default": {
            "fie": "where's fum?"
          },
          "map": {
            "Foo": "foo",
            "Bar": "bar",
            "Baz": "baz",
            "Fobe": "fobe",
            "Dt Test": "dt_test"
          },
          "remove": [
            "fobe"
          ],
          "override": {
            "state": "unknown"
          }
        }
      ],
      "terminal": {
        "smt": {
          "model": "json",
          "locus": "./test/data/output/fs/",
          "schema": "transform.json",
          "key": "*"
        },
        "options": {
          "encoding": "./test/data/input/engrams/foo_schema_t.engram.json"
        },
        "output": "./test/data/output/fs/transform.json"
      }
    },
    {
      "name": "json-csv",
      "action": "transfer",
      "origin": {
        "smt": "json|./test/data/input/|foofile.json|*"
      },
      "transforms": [
        {
          "transform": "filter",
          "match": {
            "Bar": "row"
          },
          "drop": {
            "Baz": 456
          }
        },
        {
          "transform": "mutate",
          "default": {
            "fie": "where's fum?"
          },
          "map": {
            "Foo": "foo",
            "Bar": "bar",
            "Baz": "baz",
            "Fobe": "fobe",
            "Dt Test": "dt_test"
          },
          "remove": [
            "fobe"
          ],
          "override": {
            "state": "unknown"
          }
        }
      ],
      "terminal": {
        "smt": {
          "model": "csv",
          "locus": "./test/data/output/fs/",
          "schema": "transform.csv",
          "key": "*"
        },
        "options": {
          "header": true,
          "encoding": "./test/data/input/engrams/foo_schema_t.engram.json"
        },
        "output": "./test/data/output/fs/transform.csv"
      }
    },
    {
      "name": "list_trim",
      "action": "transfer",
      "description": "select fields in directory list",
      "origin": {
        "smt": "json|./test/data/input/|file_list.json|*"
      },
      "transforms": [
        {
          "transform": "mutate",
          "select": [
            "rpath",
            "name",
            "size",
            "date"
          ]
        }
      ],
      "terminal": {
        "smt": "json|./test/data/output/fs/|file_list_trim.json|*",
        "output": "./test/data/output/fs/file_list_trim.json"
      }
    }
  ]
}