{
  "realm": "",
  "name": "transform.tract",
  "type": "tract",
  "fibers": [
    {
      "name": "csv-json",
      "action": "transfer",
      "origin": {
        "smt": "csv|./test/_data/input/|foofile.csv|*",
        "options": {
          "encoding": "./test/_data/input/engrams/foo_schema.engram.json",
          "header": true
        }
      },
      "transforms": [
        {
          "transform": "filter",
          "match": {
            "Bar": "row"
          },
          "drop": {
            "Baz": 456
          }
        },
        {
          "transform": "mutate",
          "default": {
            "fie": "where's fum?"
          },
          "map": {
            "foo": "=Foo",
            "bar": "=Bar",
            "baz": "=Baz",
            "fobe": "=Fobe",
            "dt_test": "=Dt Test"
          },
          "remove": [
            "fobe"
          ],
          "assign": {
            "state": "unknown"
          }
        }
      ],
      "terminal": {
        "smt": {
          "model": "json",
          "locus": "./test/_data/output/fs/",
          "schema": "transform.json",
          "key": "*"
        },
        "options": {
          "encoding": "./test/_data/input/engrams/foo_schema_t.engram.json"
        },
        "output": "./test/_data/output/fs/transform.json"
      }
    },
    {
      "name": "json-csv",
      "action": "transfer",
      "origin": {
        "smt": "json|./test/_data/input/|foofile.json|*",
        "options": {
          "encoding": "./test/_data/input/engrams/foo_schema.engram.json"
        }
      },
      "transforms": [
        {
          "transform": "filter",
          "match": {
            "Bar": "row"
          },
          "drop": {
            "Baz": 456
          }
        },
        {
          "transform": "mutate",
          "default": {
            "fie": "where's fum?"
          },
          "map": {
            "foo": "=Foo",
            "bar": "=Bar",
            "baz": "=Baz",
            "fobe": "=Fobe",
            "dt_test": "=Dt Test"
          },
          "remove": [
            "fobe"
          ],
          "assign": {
            "state": "unknown"
          }
        }
      ],
      "terminal": {
        "smt": {
          "model": "csv",
          "locus": "./test/_data/output/fs/",
          "schema": "transform.csv",
          "key": "*"
        },
        "options": {
          "header": true,
          "encoding": "./test/_data/input/engrams/foo_schema_t.engram.json"
        },
        "output": "./test/_data/output/fs/transform.csv"
      }
    },
    {
      "name": "list_trim",
      "action": "transfer",
      "description": "select fields in directory list",
      "origin": {
        "smt": "json|./test/_data/input/|file_list.json|*"
      },
      "transforms": [
        {
          "transform": "mutate",
          "select": [
            "rpath",
            "name",
            "size",
            "date"
          ]
        }
      ],
      "terminal": {
        "smt": "json|./test/_data/output/fs/|file_list_trim.json|*",
        "options": {
          "codify": true
        },
        "output": "./test/_data/output/fs/file_list_trim.json"
      }
    }
  ]
}
