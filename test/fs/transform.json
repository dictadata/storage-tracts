{
  "csv-json": {
    "action": "transfer",
    "origin": {
      "smt": "csv|./data/input/|foofile.csv|*",
      "options": {
        "header": true
      }
    },
    "transform": {
      "filter": {
        "match": {
          "Bar": "row"
        },
        "drop": {
          "Baz": 456
        }
      },
      "select": {
        "inject_before": {
          "fie": "where's fum?"
        },
        "inject_after": {
          "state": "unknown"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test"
        },
        "remove": [
          "fobe"
        ]
      }
    },
    "terminal": {
      "smt": {
        "model": "json",
        "locus": "./data/output/fs/",
        "schema": "transform.json",
        "key": "*"
      },
      "options": {
        "encoding": "./data/input/foo_schema_t.encoding.json"
      },
      "output": "./data/output/fs/transform.json"
    }
  },
  "json-csv": {
    "action": "transfer",
    "origin": {
      "smt": "json|./data/input/|foofile.json|*"
    },
    "transform": {
      "filter": {
        "match": {
          "Bar": "row"
        },
        "drop": {
          "Baz": 456
        }
      },
      "select": {
        "inject_before": {
          "fie": "where's fum?"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test",
          "subObj1.state": "state"
        },
        "remove": [
          "fobe"
        ]
      }
    },
    "terminal": {
      "smt": {
        "model": "csv",
        "locus": "./data/output/fs/",
        "schema": "transform.csv",
        "key": "*"
      },
      "options": {
        "header": true,
        "encoding": "./data/input/foo_schema_t.encoding.json"
      },
      "output": "./data/output/fs/transform.csv"
    }
  },
  "list_trim": {
    "action": "transfer",
    "description": "select fields in directory list",
    "origin": {
      "smt": "json|./data/input/|file_list.json|*"
    },
    "transforms": {
      "select": {
        "fields": [
          "rpath",
          "name",
          "size",
          "date"
        ]
      }
    },
    "terminal": {
      "smt": "json|./data/output/fs/|file_list_trim.json|*",
      "output": "./data/output/fs/file_list_trim.json"
    }
  }
}
