{
  "list_local": {
    "action": "list",
    "origin": {
      "smt": "json|ftp://dev.dictadata.net/data/|foofile_*.json|*",
      "options": {
        "recursive": true
      }
    },
    "terminal": {
      "output": "./data/output/ftp/list.json"
    }
  },
  "list_tiger": {
    "action": "list",
    "origin": {
      "smt": "json|ftp://ftp2.census.gov/geo/tiger/TIGER2020/|*.pdf|*",
      "options": {
        "recursive": false
      }
    },
    "terminal": {
      "output": "./data/output/ftp/list_tiger.json"
    }
  }
}
