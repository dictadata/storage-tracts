{
  "json-index": {
    "action": "transfer",
    "origin": {
      "smt": "json|./data/input/|foofile.json|*",
      "options": {
        "encoding": "./data/input/foo_schema.encoding.json"
      }
    },
    "terminal": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://localhost:9200",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  "index-json": {
    "action": "transfer",
    "origin": {
      "smt": "elasticsearch-foo_schema",
      "pattern": {
        "order": {
          "Foo": "asc"
        }
      }
    },
    "terminal": {
      "smt": "json|./data/output/elasticsearch/|foofile.json|*",
      "output": "./data/output/elasticsearch/foofile.json"
    }
  }
}
