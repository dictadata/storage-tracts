{
  "json-index": {
    "action": "transfer",
    "origin": {
      "smt": "json|./data/input/|foofile.json|*",
      "options": {
        "encoding": "./data/input/foo_schema.encoding.json"
      }
    },
    "terminal": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://dev.dictadata.org:9200",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  "index-json": {
    "action": "transfer",
    "origin": {
      "smt": "foo:elasticsearch-foo_schema",
      "pattern": {
        "order": {
          "Foo": "asc"
        }
      }
    },
    "terminal": {
      "smt": "json|./data/output/elasticsearch/|transfer_foofile.json|*",
      "output": "./data/output/elasticsearch/transfer_foofile.json"
    }
  },
  "fulltext": {
    "action": "transfer",
    "origin": {
      "smt": "foo:elasticsearch-foo_schema",
      "pattern": {
        "match": {
          "Bar": {
            "search": "row boat"
          }
        }
      }
    },
    "terminal": {
      "smt": "json|./data/output/elasticsearch/|transfer_fulltext.json|*",
      "output": "./data/output/elasticsearch/transfer_fulltext.json"
    }
  },
  "multifield": {
    "action": "transfer",
    "origin": {
      "smt": "foo:elasticsearch-foo_schema",
      "pattern": {
        "match": {
          "_": {
            "search": "first row boat",
            "fields": [
              "Foo",
              "Bar"
            ]
          }
        }
      }
    },
    "terminal": {
      "smt": "json|./data/output/elasticsearch/|transfer_multifield.json|*",
      "output": "./data/output/elasticsearch/transfer_multifield.json"
    }
  }
}
