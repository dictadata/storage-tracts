{
  "json-etl2": {
    "action": "transfer",
    "origin": {
      "smt": "json|./test/data/input/|foofile.json|*"
    },
    "terminal": {
      "smt": "elasticsearch|http://localhost:9200|foo_schema_etl2|=Foo",
      "options": {
        "encoding": "./test/data/input/foo_schema_t.encoding.json"
      }
    },
    "transform": {
      "filter": {
        "match": { "Bar": "row" },
        "drop": {
          "Baz": { "eq": 456 }
        }
      },
      "select": {
        "inject_after": {
          "fie": "where's fum?"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test",
          "subObj1.state": "state"
        },
        "remove": ["fobe"]
      }
    }
  }
}
