{
  "json-etl2": {
    "action": "transfer",
    "origin": {
      "smt": "json|./data/input/|foofile.json|*",
      "options": {
        "encoding": "./data/input/foo_schema.encoding.json"
      }
    },
    "terminal": {
      "smt": "elasticsearch|http://dev.dictadata.org:9200|foo_schema_etl2|=Foo",
      "options": {
        "encoding": "./data/input/foo_schema_t.encoding.json"
      }
    },
    "transform": {
      "filter": {
        "match": {
          "Bar": "row"
        },
        "drop": {
          "Baz": {
            "eq": 456
          }
        }
      },
      "select": {
        "inject_after": {
          "fie": "where's fum?"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test",
          "subObj1.state": "state"
        },
        "remove": [
          "fobe"
        ]
      }
    }
  }
}
