{
  "load": {
    "action": "scan",
    "origin": {
      "smt": "json|./data/input/|foofile*.json|*",
      "options": {
        "recursive": false
      }
    },
    "schema": {
      "action": "transfer",
      "origin": {
        "smt": "json|./data/input/|${name}|*",
        "options": {
          "encoding": "./data/input/foo_schema_01.encoding.json"
        }
      },
      "terminal": {
        "smt": "elasticsearch|http://localhost:9200|foo_load|!Foo",
        "options": {
          "encoding": "./data/input/foo_schema_01.encoding.json"
        }
      }
    },
    "terminal": {}
  }
}
