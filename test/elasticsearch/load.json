{
  "load": {
    "action": "scan",
    "origin": {
      "smt": "json|./test/data/input/|foofile*.json|*",
      "options": {
        "recursive": false,
        "replace_terminal_smt": false
      }
    },
    "schema": {
      "action": "transfer",
      "origin": {
        "smt": "json|./test/data/input/|*|*",
        "options": {
          "encoding": "./test/data/input/foo_schema.encoding.json"
        }
      },
      "terminal": {
        "smt": "elasticsearch|http://localhost:9200|foo_load|*",
        "options": {
          "encoding": "./test/data/input/foo_schema.encoding.json"
        }
      }
    },
    "terminal": {}
  }
}
