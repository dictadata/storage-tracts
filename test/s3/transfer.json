{
  "local-s3": {
    "action": "transfer",
    "origin": {
      "smt": "json|./test/data/input/|foofile.json|*"
    },
    "terminal": {
      "smt": {
        "model": "json",
        "locus": "s3:dictadata.org/data/test/output/",
        "schema": "s3_foofile.json",
        "key": "*"
      },
      "options": {
        "s3": {
          "aws_profile": "dictadata"
        }
      }
    }
  },
  "s3-local": {
    "action": "transfer",
    "origin": {
      "smt": "json|s3:dictadata.org/data/test/input/|foofile.json|*",
      "options": {
        "s3": {
          "aws_profile": "dictadata"
        }
      }
    },
    "terminal": {
      "smt": "json|./test/data/output/s3/|foofile.json|*"
    }
  }
}