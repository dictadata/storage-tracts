{
  "shp-json": {
    "action": "transfer",
    "origin": {
      "smt": "shp|./test/data/shapes/|polygons|*",
      "options": {}
    },
    "terminal": {
      "smt": "json|./test/data/output/shapefile/|polygons.json|*"
    }
  },
  
  "zip-json": {
    "action": "transfer",
    "origin": {
      "smt": "shp|zip:./test/data/shapes/points.zip|points|*",
      "options": {}
    },
    "terminal": {
      "smt": "json|./test/data/output/shapefile/|points.json|*"
    }
  },

  "points-elastic": {
    "action": "transfer",
    "origin": {
      "smt": "shp|zip:./test/data/shapes/points.zip|points|*",
      "options": {}
    },
    "terminal": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://localhost:9200",
        "schema": "shapes",
        "key": "*"
      }
    }
  },

  "polygons-elastic": {
    "action": "transfer",
    "origin": {
      "smt": "shp|./test/data/shapes/|polygons|*",
      "options": {}
    },
    "terminal": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://localhost:9200",
        "schema": "shapes",
        "key": "*"
      }
    }
  },

  "elastic-json": {
    "action": "transfer",
    "origin": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://localhost:9200",
        "schema": "shapes",
        "key": "*"
      },
      "options": {}
    },
    "terminal": {
      "smt": "json|./test/data/output/shapefile/|shapes.json|*"
    }
  },

  "download": {
    "action": "copy",
    "origin": {
      "smt": "*|ftp://anonymous:anonymous@ftp2.census.gov/geo/tiger/TIGER2020/COUNTY/|*.zip|*",
      "options": {}
    },
    "terminal": {
      "smt": "*|file:c:/var/data/www2.census.gov/|*|*",
      "options": {
        "keep_rpath": true
      }
    }
  },

  "tl_2020_us_state": {
    "action": "transfer",
    "origin": {
      "smt": "shp|zip:/var/data/www2.census.gov/geo/tiger/TIGER2020/STATE/tl_2020_us_state.zip|tl_2020_us_state|*"
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|tl_2020_us_state.json|*"
    }
  },

  "el_json": {
    "action": "transfer",
    "origin": {
      "smt": "elasticsearch|http://localhost:9200|census_2020|*"
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|tl_2020_us_state_elastic.json|*"
    }
  }
}
