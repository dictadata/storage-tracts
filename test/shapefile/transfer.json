{
  "shp-json": {
    "action": "transfer",
    "origin": {
      "smt": "shp|./data/input/shapes/|polygons|*",
      "options": {}
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|polygons.json|*",
      "output": "./data/output/shapefile/polygons.json"
    }
  },
  "zip-json": {
    "action": "transfer",
    "origin": {
      "smt": "shp|zip:./data/input/shapes/points.zip|points|*",
      "options": {}
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|points.json|*",
      "output": "./data/output/shapefile/points.json"
    }
  },
  "points-elastic": {
    "action": "transfer",
    "origin": {
      "smt": "shp|zip:./data/input/shapes/points.zip|points|*",
      "options": {}
    },
    "transform": {
      "mutate": {
        "default": {
          "id": "=geometry.type+properties.FID"
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://dev.dictadata.org:9200",
        "schema": "shapes",
        "key": "!id"
      }
    }
  },
  "polygons-elastic": {
    "action": "transfer",
    "origin": {
      "smt": "shp|./data/input/shapes/|polygons|*",
      "options": {}
    },
    "transform": {
      "mutate": {
        "default": {
          "id": "=geometry.type+properties.FID"
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://dev.dictadata.org:9200",
        "schema": "shapes",
        "key": "!id"
      }
    }
  },
  "elastic-json": {
    "action": "transfer",
    "origin": {
      "smt": {
        "model": "elasticsearch",
        "locus": "http://dev.dictadata.org:9200",
        "schema": "shapes",
        "key": "!id"
      },
      "pattern": {
        "order": {
          "id": "desc"
        }
      }
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|shapes.json|*",
      "output": "./data/output/shapefile/shapes.json"
    }
  },
  "download": {
    "action": "copy",
    "origin": {
      "smt": "*|ftp://dev.dictadata.org/data/US/census.gov/geo/tiger/TIGER2020/STATE/|*.zip|*",
      "options": {}
    },
    "terminal": {
      "smt": "*|file:c:/var/data/US/census.gov/|*|*",
      "options": {
        "use_rpath": true
      }
    }
  },
  "tl_2020_us_state": {
    "action": "transfer",
    "origin": {
      "smt": "shp|zip:/var/data/US/census.gov/geo/tiger/TIGER2020/STATE/tl_2020_us_state.zip|tl_2020_us_state|*"
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|tl_2020_us_state.json|*"
    }
  },
  "el_json": {
    "action": "transfer",
    "origin": {
      "smt": "elasticsearch|http://dev.dictadata.org:9200|tl_2020_us_state|*",
      "pattern": {
        "match": {
          "geometry": {
            "contains": [
              -73.985428,
              40.748817
            ]
          }
        }
      }
    },
    "terminal": {
      "smt": "json|./data/output/shapefile/|tl_2020_us_state_NY.json|*"
    }
  }
}
